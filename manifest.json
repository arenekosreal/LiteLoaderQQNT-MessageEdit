{
    "manifest_version": 3,
    "type": "extension",
    "name": "QQ消息修改",
    "slug": "MessageEdit",
    "description": "聊天界面，右键点击文本消息，选择【编辑消息内容】编辑即可",
    "version": "1.0.3",
    "thumbnail": "./icon.png",
    "author": {
        "name": "MIMO",
        "link": "https://github.com/mimocvb"
    },
    "repository": {
        "repo": "mimocvb/LiteLoaderQQNT-MessageEdit",
        "branch": "v3",
        "use_release": {
            "tag": "latest",
            "name": "MessageEdit.zip"
        }
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}
